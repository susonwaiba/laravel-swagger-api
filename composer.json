{
    "name":"hcesrl/laravel-swagger-api",
    "description":"API Swagger package for Laravel.",
    "homepage":"https://github.com/HCESrl/laravel-swagger-api",
    "license":"MIT",
    "authors":[
        {
            "name":"Fabio Savina",
            "email":"fabio.savina@gmail.com"
        }
    ],
    "keywords":[
        "laravel",
        "swagger",
        "api"
    ],
    "require":{
        "calcinai/strut":"^0.2",
        "swagger-api/swagger-ui":"^3.9"
    },
    "suggest":{
        "barryvdh/laravel-cors":"Required to send CORS headers (^0.10.0)"
    },
    "autoload":{
        "psr-4":{
            "LaravelApi\\":"src"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "LaravelApi\\Tests\\":"tests/"
        }
    },
    "extra":{
        "laravel":{
            "providers":[
                "LaravelApi\\ServiceProvider"
            ],
            "aliases":{
                "Api":"LaravelApi\\Facade"
            }
        }
    }
}
